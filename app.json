{
  "expo": {
    "name": "ShopSavvy1",
    "slug": "ShopSavvy1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "permissions": [
        "CAMERA"
      ]
    },
    "android": {
      "package": "com.exemple.ShopSavvy1",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "CAMERA"
        ]
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
